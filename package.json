{
  "name": "analytica.click.gh-build",
  "version": "2",
  "description": "",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": " <help@analytica.click> (analytica.click)",
  "license": "ISC",
  "private": false,
  "scripts": {
    "format": "eslint --ext .ts,.tsx ./src --fix",
    "lint": "tsc && eslint --ext .ts,.tsx ./src",
    "build-direct": "rimraf .build && rimraf dist && cross-env NODE_ENV=prod ts-node esbuild",
    "build": "pnpm run lint && pnpm build-direct",
    "start": "ts-node src/direct.ts"
  },
  "dependencies": {
    "@actions/core": "1.10.1",
    "@types/node": "20.11.5",
    "analytica.click": "0.0.419",
    "cross-env": "7.0.3",
    "cross-fetch": "4.0.0",
    "esbuild": "0.19.11",
    "eslint-config-e7npm": "0.0.77",
    "rimraf": "5.0.5",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=18",
    "yarn": "use pnpm",
    "npm": "use pnpm",
    "pnpm": ">=8.6"
  }
}